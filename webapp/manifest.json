{
  "_version": "1.73.1",
  "sap.app": {
    "id": "productsovp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:ovp",
      "version": "1.19.3",
      "toolsId": "4e9190b8-da20-4def-8e89-66095f3b719a"
    },
    "dataSources": {
      "ZRP_UI_OVP_O2_VAN": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZRP_UI_OVP_O2_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/mainService/ZRP_UI_OVP_O2_VAN.xml"
        }
      },
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZRP_UI_OVP_O2/",
        "type": "OData",
        "settings": {
          "annotations": [
            "ZRP_UI_OVP_O2_VAN",
            "annotation"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "products-display_ovp": {
          "semanticObject": "products",
          "action": "display_ovp",
          "title": "{{products-display_ovp.flpTitle}}",
          "subTitle": "{{products-display_ovp.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.136.8",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.f": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.suite.ui.generic.template": {},
        "sap.ovp": {},
        "sap.ui.rta": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "productsovp.i18n.i18n"
        }
      },
      "mainModel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false,
          "useBatch": false,
          "metadataUrlParams": {
            "sap-value-list": "none"
          }
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [],
      "targets": {}
    }
  },
  "sap.ovp": {
    "globalFilterModel": "mainModel",
    "globalFilterEntitySet": "GlobalFilters",
    "containerLayout": "resizable",
    "enableLiveFilter": true,
    "considerAnalyticalParameters": false,
    "cards": {
      "OrdersQuantity": {
        "model": "mainModel",
        "template": "sap.ovp.cards.charts.analytical",
        "settings": {
          "title": "Orders Quantity",
          "entitySet": "Cards",
          "tabs": [
            {
              "value": "By Countries",
              "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
              "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#dpOrdQuanByCountry",
              "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#pvOrdQuanByCountry",
              "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#OrdQuanByCountry"
            },
            {
              "value": "By Product Groups",
              "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
              "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#dpOrdQuanByProdGr",
              "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#pvOrdQuanByProdGr",
              "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#OrdQuanByProdGr"
            }
          ]
        }
      },
      "AVGIncomeCountry": {
        "model": "mainModel",
        "template": "sap.ovp.cards.charts.analytical",
        "settings": {
          "title": "Average Gross Income",
          "subtTitle": "",
          "entitySet": "Cards",
          "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
          "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#dpAVGIncomeCountry",
          "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#pvAVGIncomeCountry",
          "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#AVGIncomeCountry"
        }
      },
      "OrdersList": {
        "model": "mainModel",
        "template": "sap.ovp.cards.list",
        "settings": {
          "title": "Profitable of Orders",
          "subTitle": "",
          "entitySet": "Cards",
          "listType": "extended",
          "listFlavor": "bar",
          "addODataSelect": false,
          "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#dpAVGIncomeCountryList",
          "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#pvOrdersList",
          "annotationPath": "com.sap.vocabularies.UI.v1.LineItem#OrdersList",
          "defaultSpan": {
            "rows": 7,
            "cols": 1
          }
        }
      },
      "OrdersList2": {
        "model": "mainModel",
        "template": "sap.ovp.cards.list",
        "settings": {
          "title": "Profitable of Orders",
          "entitySet": "Cards",
          "listType": "condensed",
          "listFlavor": "bar",
          "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
          "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#dpAVGIncomeCountry",
          "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#pvOrdersList",
          "annotationPath": "com.sap.vocabularies.UI.v1.LineItem#OrdersList",
          "defaultSpan": {
            "rows": 10,
            "cols": 1
          }
        }
      },
      "StackOrdersList": {
        "model": "mainModel",
        "template": "sap.ovp.cards.stack",
        "settings": {
          "title": "Orders",
          "subTitle": "General Information",
          "sortBy": "OrderID",
          "sortOrder": "ascending",
          "entitySet": "Cards",
          "addODataSelect": "false",
          "objectStreamCardsSettings": {
            "annotationPath": "com.sap.vocabularies.UI.v1.Facets#StackOrdersList"
          },
          "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification#FaceSide,com.sap.vocabularies.UI.v1.Identification#FlipSide"
        }
      }
    },
    "chartSettings": {}
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "analytical"
  }
}